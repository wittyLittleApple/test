{
  "name": "study",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/wittyLittleApple/study.git",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.0.1",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-tabs": "off",
      "no-extend-native": "off",
      "camelcase": "off",
      "no-new": "off",
      "no-undef-init": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/ban-ts-ignore": "off"
    },
    "extends": [
      "standard",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "parser": [
        "@typescript-eslint/parser",
        "babel-eslint"
      ]
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-static": "^1.2.6",
    "jade": "^1.11.0"
  }
}
